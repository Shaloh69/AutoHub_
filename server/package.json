{
  "name": "car-marketplace-api",
  "version": "1.0.0",
  "description": "Car Marketplace Philippines API with subscription system",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "dev:debug": "nodemon --exec ts-node --inspect src/app.ts",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles",
    "test:integration": "jest --testPathPattern=integration --detectOpenHandles",
    "test:unit": "jest --testPathPattern=unit --detectOpenHandles",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "migration:generate": "npm run build && npx typeorm migration:generate -d dist/config/database.js",
    "migration:create": "npx typeorm migration:create",
    "migration:run": "npm run build && npx typeorm migration:run -d dist/config/database.js",
    "migration:revert": "npm run build && npx typeorm migration:revert -d dist/config/database.js",
    "migration:show": "npm run build && npx typeorm migration:show -d dist/config/database.js",
    "schema:sync": "npm run build && npx typeorm schema:sync -d dist/config/database.js",
    "schema:drop": "npm run build && npx typeorm schema:drop -d dist/config/database.js",
    "seed": "npm run build && node dist/database/seeds/index.js",
    "seed:dev": "ts-node src/database/seeds/index.ts",
    "db:reset": "npm run migration:revert && npm run migration:run && npm run seed",
    "docker:build": "docker build -t car-marketplace-api .",
    "docker:run": "docker run -p 3000:3000 car-marketplace-api",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up -d",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version",
    "deploy:staging": "npm run build && npm run test && echo 'Deploy to staging'",
    "deploy:production": "npm run build && npm run test && echo 'Deploy to production'",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "logs:error": "pm2 logs --err",
    "logs:out": "pm2 logs --out",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "backup:db": "mysqldump -u root -p car_marketplace_ph > backup_$(date +%Y%m%d_%H%M%S).sql",
    "analyze:bundle": "npm run build && npx webpack-bundle-analyzer dist/bundle.js",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "docs:generate": "typedoc --out docs src",
    "docs:serve": "npx http-server docs -p 8080"
  },
  "keywords": [
    "car-marketplace",
    "philippines",
    "api",
    "typescript",
    "nodejs",
    "express",
    "socket.io",
    "subscription",
    "stripe"
  ],
  "author": "Car Marketplace Philippines Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/car-marketplace-ph-api"
  },
  "dependencies": {
    "express": "^4.18.2",
    "typeorm": "^0.3.17",
    "mysql2": "^3.6.0",
    "socket.io": "^4.7.2",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "stripe": "^13.6.0",
    "multer": "^1.4.5-lts.1",
    "joi": "^17.9.2",
    "reflect-metadata": "^0.1.13",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^6.8.1",
    "compression": "^1.7.4",
    "redis": "^4.6.7",
    "nodemailer": "^6.9.4",
    "twilio": "^4.14.0",
    "aws-sdk": "^2.1419.0",
    "cloudinary": "^1.38.0",
    "sharp": "^0.32.4",
    "cron": "^2.4.4",
    "winston": "^3.10.0",
    "axios": "^1.4.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "slugify": "^1.6.6",
    "validator": "^13.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.5",
    "@types/express": "^4.17.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/cors": "^2.8.13",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/joi": "^17.2.3",
    "@types/compression": "^1.7.2",
    "@types/nodemailer": "^6.4.8",
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "@types/validator": "^13.9.0",
    "typescript": "^5.1.6",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "jest": "^29.6.1",
    "@types/jest": "^29.5.3",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.12",
    "eslint": "^8.45.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "prettier": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0",
    "typedoc": "^0.24.8",
    "webpack-bundle-analyzer": "^4.9.0",
    "cross-env": "^7.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,js}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}